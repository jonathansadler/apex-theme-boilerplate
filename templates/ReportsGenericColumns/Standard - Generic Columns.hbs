{{#row_template_before_rows}}
<table id="report_#REGION_STATIC_ID#" class="#COMPONENT_CSS_CLASSES#" #REPORT_ATTRIBUTES#>
{{/row_template_before_rows}}

{{#before_column_heading}}
<thead>
<tr>
{{/before_column_heading}}

    {{#column_heading_template}}
    <th id="#COLUMN_HEADER_NAME#" #ALIGNMENT# #COLUMN_WIDTH#>#COLUMN_HEADER#</th>
    {{/column_heading_template}}

{{#after_column_heading}}
</tr>
</thead>
<tbody>
{{/after_column_heading}}

    {{#row_template_before_first}}
    <tr>
    {{/row_template_before_first}}

        {{#row_template1}}
        <td headers="#COLUMN_HEADER_NAME#" #ALIGNMENT#>#COLUMN_VALUE#</td>
        {{/row_template1}}
        {{#row_template_condition1}}{{/row_template_condition1}}
        {{#row_template_display_cond1}}{{/row_template_display_cond1}}
        {{#row_template2}}{{/row_template2}}
        {{#row_template_condition2}}{{/row_template_condition2}}
        {{#row_template_display_cond2}}{{/row_template_display_cond2}}
        {{#row_template3}}{{/row_template3}}
        {{#row_template_condition3}}{{/row_template_condition3}}
        {{#row_template_display_cond3}}{{/row_template_display_cond3}}
        {{#row_template4}}{{/row_template4}}
        {{#row_template_condition4}}{{/row_template_condition4}}
        {{#row_template_display_cond4}}{{/row_template_display_cond4}}

    {{#row_template_after_last}}
    </tr>
    {{/row_template_after_last}}

{{#row_template_after_rows}}
</tbody>
</table>
<tfoot>
    <div>#EXTERNAL_LINK##CSV_LINK#</div>
    <div>#PAGINATION#</div>
</tfoot>
{{/row_template_after_rows}}

{{#pagination_template}}
<span>#TEXT#</span>
{{/pagination_template}}
{{#next_page_template}}
<a href="#LINK#">next</a>
{{/next_page_template}}
{{#previous_page_template}}
<a href="#LINK#">previous</a>
{{/previous_page_template}}
{{#next_set_template}}
<a href="#LINK#">next set</a>
{{/next_set_template}}
{{#previous_set_template}}
<a href="#LINK#">previous set</a>
{{/previous_set_template}}

